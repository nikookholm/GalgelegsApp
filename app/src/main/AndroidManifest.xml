<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="s112011.galgelegsapp">

    <uses-sdk
        android:maxSdkVersion="23"
        android:minSdkVersion="14"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />

    <application
        android:name=".GalgeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="g24 Galgeleg"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".Login"
            android:label="Login"></activity>
        <activity
            android:name=".MainManuActivity"
            android:label="Hovedmenu" />
        <activity android:name=".Spil"
            android:label="Spil"/>
        <activity
            android:name=".Indstillinger_Activity"
            android:label="Indstillinger" />
        <activity android:name=".HighScores_Activity" />
        <activity android:name=".Feedback_Activity" />
        <activity android:name=".Opret_Bruger"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>
